{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php@0.7.1"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.php"
        },
        {
            "src": "/add.php",
            "dest": "api/add.php"
        }
    ]
}